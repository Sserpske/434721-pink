{
  "name": "pink",
  "version": "15.0.0",
  "private": true,
  "description": "Личный проект «Пинк» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.1.x",
    "browser-sync": "2.24.x",
    "gulp": "4.0.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-sass": "4.0.x",
    "gulp-webp": "^3.0.0",
    "node-normalize-scss": "^8.0.0",
    "stylelint": "9.6.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp css",
    "svg-sprite": "gulp svg-sprite",
    "start": "gulp start",
    "webp": "gulp webp"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.12"
  },
  "dependencies": {
    "gulp-cheerio": "^0.6.3",
    "gulp-replace": "^1.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgmin": "^2.1.0",
    "svgxuse": "^1.2.6"
  }
}
